#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void)
{
  char str1[] = "programming";
  char str2[100];

  my_strcpy(str2,str1);
  printf("%s\n",str2);

  return 0;
}

void my_strcpy(char target[],char source[])
{
  char* tar = target; char* sou = source;
  while(*tar++ = *sou++);
}
