#include <stdio.h>
#include <string.h>

int main(void)
{

  char barcode[100];
  char str[100];
  
  printf("バーコードを読み込ませてください\n");
  scanf("%s",barcode);

  int len;

  len = strlen(barcode) ;

  int i;

  for(i = 0;i <= len;i++){
    str[i] = barcode[len - 1];
  }

  printf("%s",barcode);
  printf("%s",str);

  return 0;

}

  

  
