#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main(void)
{

  int num[20];

  srand((unsigned)time(NULL));
  
  int i;
  for(i = 0;i < 20;i++){
    num[i] = rand()%100;
    printf("[%d] = %d\n",i,num[i]);
  }
  
  int cut_num;
  printf("How many do you want cut?\n");
  scanf("%d",&erace_num);

  int cut_index[];
  printf("Please put number.\n");
  for(i = 0;i < cut_num;i++){
    printf("cut[%d]=");
    scanf("%d",cut_index);
  }

  for(i = 0;i < 20;i++){
    if(i != cut_index[]){
      printf("box[%d]=%d\n",i,num[i]);
    }
  }



  return 0;

}
  
