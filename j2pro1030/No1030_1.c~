#include <stdio.h>
#include <stdlib.h>

#define SENT "aaa"

int my_strlen(char *str);

int main(void)
{

  char str[]= SENT;

  int lengthl;

  my_strlen(str);

  printf("length = %d",length);

  exit(EXIT_SUCCESS);

}

int my_strlen(char *str)
{
  int len = 0;
  while(*str++)len++;
  return len;
}

